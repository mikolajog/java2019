<AnchorPane fx:id="anchorpane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="calculator.Controller">
   <children>
      <TextField fx:id="num1Field" layoutX="38.0" layoutY="40.0" />
      <TextField fx:id="num2Field" layoutX="376.0" layoutY="40.0" />
      <Label layoutX="444.0" layoutY="24.0" text="Num2" />
      <Label layoutX="106.0" layoutY="24.0" text="Num1" />
      <TextField fx:id="ResultField" layoutX="215.0" layoutY="270.0" />
      <Label layoutX="281.0" layoutY="254.0" text="Result" />
      <Button fx:id="plusButton" layoutX="288.0" layoutY="97.0" mnemonicParsing="false" onAction="#anyButtonClick" text="+" />
      <Button fx:id="minusButton" layoutX="291.0" layoutY="145.0" mnemonicParsing="false" onAction="#anyButtonClick" text="-" />
